= form_with model: @menu, local: true do |f|
  = f.label :name do
    name
  = f.text_field :name
  = f.label :price do
    price
  = f.text_field :price
  = f.label :comment do
    comment
  = f.text_area :comment
  = f.collection_check_boxes :category_ids, Category.all, :id, :name
  = f.submit "送信"
